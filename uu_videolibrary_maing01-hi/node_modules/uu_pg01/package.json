  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "create-react-class": "15.6.3",
    "prop-types": "15.7.2",
    "uu5g04": "^1.0.0",
    "uu_contentkitg01": "^2.0.0"
  },
  "uuBuildSettings": {
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM",
      "create-react-class": "createReactClass",
      "prop-types": "PropTypes",
      "uu5g04": {
        "globalVariable": "UU5",
        "uri": ""
      },
      "uu5g04-bricks": {
        "globalVariable": "UU5.Bricks",
        "uri": ""
      },
      "uu5g04-forms": {
        "globalVariable": "UU5.Forms",
        "uri": ""
      },
      "uu5g04-block-layout": {
        "globalVariable": "UU5.BlockLayout",
        "uri": ""
      },
      "uu_contentkitg01": {
        "globalVariable": "UuContentKit",
        "uri": ""
      },
      "uu_pg01": "UuP",
      "uu_pg01-bricks": "UuP.Bricks",
      "uu_pg01-forms": "UuP.Forms",
      "uu_pg01-tiles": "UuP.Tiles"
    },
    "packs": [
      {
        "entryPoints": [
          "index.js"
        ],
        "outputFile": "uu_pg01.js",
        "libraryGlobalVariable": "UuP"
      },
      {
        "entryPoints": [
          "bricks/bricks-build.js"
        ],
        "outputFile": "uu_pg01-bricks.js",
        "libraryGlobalVariable": "UuP.Bricks"
      },
      {
        "entryPoints": [
          "forms/forms-build.js"
        ],
        "outputFile": "uu_pg01-forms.js",
        "libraryGlobalVariable": "UuP.Forms"
      },
      {
        "entryPoints": [
          "tiles/tiles-build.js"
        ],
        "outputFile": "uu_pg01-tiles.js",
        "libraryGlobalVariable": "UuP.Tiles"
      }
    ]
  },
  "devDependencies": {
    "uu_appg01_devkit": "^4.0.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "clean": "uu_appg01_devkit clean",
    "test": "uu_appg01_devkit test",
    "build": "uu_appg01_devkit build",
    "package": "uu_appg01_devkit package && node tools/postpackage.js",
    "dist": "uu_appg01_devkit package",
    "start": "uu_appg01_devkit start",
    "prettify": "uu_appg01_devkit prettify",
