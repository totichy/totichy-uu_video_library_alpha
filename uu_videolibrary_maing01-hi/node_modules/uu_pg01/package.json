    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^uu_pg01$": "<rootDir>/src/index.js",
      "^uu_pg01-([^/?!]+)": "<rootDir>/src/$1/$1-build.js",
      "^uu5g04-([^/?!]+)$": "uu5g04/dist-node/$1/$1-build.js",
      "^uu_plus4u5g01$": "uu_plus4u5g01/dist-node/index.js",
      "^uu_plus4u5g01-([^/?!]+)$": "uu_plus4u5g01/dist-node/$1/$1-build.js",
