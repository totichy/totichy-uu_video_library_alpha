{
  "name": "uu5tilesg01",
  "version": "3.6.1",
  "description": "UU5.Tiles.Tile and List Library",
  "license": "License Commercial",
  "namespace": "UU5.Tiles",
  "spec": "uu5-lib; uu_appg01@1.3.0",
  "dependencies": {
    "memoize-one": "5.0.4",
    "react-dnd": "7.0.2",
    "react-dnd-html5-backend": "7.0.2",
    "react-dnd-touch-backend": "0.7.1",
    "react-virtualized": "9.20.1",
    "uu5g04": "^1.19.0"
  },
  "uuBuildSettings": {
    "externals": {
      "react-dnd": {
        "globalVariable": "ReactDnD",
        "uri": "https://cdn.plus4u.net/libs/react-dnd/7.0.2/ReactDnD.min.js"
      },
      "react-dnd-touch-backend": {
        "globalVariable": "reactDndTouchBackend",
        "uri": "https://cdn.plus4u.net/libs/react-dnd-touch-backend/0.7.1/Touch.browserified.js"
      },
      "react-dnd-html5-backend": {
        "globalVariable": "ReactDnDHTML5Backend",
        "uri": "https://cdn.plus4u.net/libs/react-dnd-html5-backend/7.0.2/ReactDnDHTML5Backend.min.js"
      }
    },
    "devServerMiddlewares": [
      "uu5g04/tools/dev-server/doc-middleware.js"
    ]
  },
  "devDependencies": {
    "uu_appg01_devkit": "^1.3.0",
    "uu_appg01": "4.5.2"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "clean": "uu_appg01_devkit clean",
    "test": "uu_appg01_devkit test",
    "build": "uu_appg01_devkit build",
    "package": "uu_appg01_devkit package",
    "dist": "uu_appg01_devkit package",
    "start": "uu_appg01_devkit start",
    "prettify": "uu_appg01_devkit prettify",
    "deploy": "uu_appg01_devkit deploy",
    "uuAppBox": "uu_appg01_devkit uuAppBox",
    "uuCloudDeploy": "uu_appg01_devkit uuCloudDeploy",
    "uuCloudUndeploy": "uu_appg01_devkit uuCloudUndeploy",
    "uuCloudDeployList": "uu_appg01_devkit uuCloudDeployList",
    "docUploadFiles": "node node_modules/uu5g04/tools/doc-cmd/update.js files -y",
    "generate": "uu_appg01_devkit generate"
  },
  "main": "dist-node/index.js",
  "jsnext:main": "dist-esm/index.js",
  "module": "dist-esm/index.js",
  "files": [
    "dist*"
  ],
  "prettier": {
    "printWidth": 120
  }
}