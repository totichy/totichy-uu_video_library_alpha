        ],
        "libraryGlobalVariable": "UU5.CodeKit",
        "outputFile": "uu5codekitg01.js"
      },
      {
        "entryPoints": [
          "index-markdown.js"
        ],
        "libraryGlobalVariable": "UU5.CodeKitMarkdown",
        "outputFile": "uu5codekitg01-markdown.js"
      },
      {
