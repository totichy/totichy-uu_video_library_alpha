{
  "name": "uu_plus4u5g01",
  "version": "4.19.2",
  "description": "uuPlus4U5 Library.",
  "license": "License Commercial",
  "namespace": "Plus4U5",
  "spec": "uu5-lib; uu_appg01_devkit@4.0.0",
  "dependencies": {
    "memoize-one": "5.0.5",
    "react-dnd": "7.0.2",
    "react-dnd-html5-backend": "7.0.2",
    "react-dnd-touch-backend": "0.7.1",
    "uu_appg01": "^5.0.0",
    "uu_appg01_core": "^5.0.0",
    "uu_appg01_oidc": "^3.0.0",
    "uu5g04": "^1.43.0"
  },
  "uuBuildSettings": {
    "packs": [
      {
        "entryPoints": [
          "core/core.js"
        ],
        "libraryGlobalVariable": "Plus4U5",
        "outputFile": "uu_plus4u5g01.js",
        "banner": "./tools/build-add-plus4u5g02-banner.js"
      },
      {
        "entryPoints": [
          "app/app-build.js"
        ],
        "libraryGlobalVariable": "Plus4U5.App",
        "outputFile": "uu_plus4u5g01-app.js",
        "banner": "./tools/build-add-plus4u5g02-banner.js"
      },
      {
        "entryPoints": [
          "bricks/bricks-build.js"
        ],
        "libraryGlobalVariable": "Plus4U5.Bricks",
        "outputFile": "uu_plus4u5g01-bricks.js",
        "banner": "./tools/build-add-plus4u5g02-banner.js"
      },
      {
        "entryPoints": [
          "uubmldraw/uubmldraw-build.js"
        ],
        "libraryGlobalVariable": "Plus4U5.UuBmlDraw",
        "outputFile": "uu_plus4u5g01-uubmldraw.js",
        "banner": "./tools/build-add-plus4u5g02-banner.js"
      },
      {
        "entryPoints": [
          "hooks/hooks-build.js"
        ],
        "libraryGlobalVariable": "Plus4U5.Hooks",
        "outputFile": "uu_plus4u5g01-hooks.js",
        "banner": "./tools/build-add-plus4u5g02-banner.js"
      },
      {
        "entryPoints": [
          "context/context-build.js"
        ],
        "libraryGlobalVariable": "Plus4U5.Context",
        "outputFile": "uu_plus4u5g01-context.js",
        "banner": "./tools/build-add-plus4u5g02-banner.js"
      }
    ],
    "externals": {
      "react-dnd": {
        "globalVariable": "ReactDnD",
        "uri": "https://cdn.plus4u.net/libs/react-dnd/7.0.2/ReactDnD.min.js"
      },
      "react-dnd-touch-backend": {
        "globalVariable": "reactDndTouchBackend",
        "uri": "https://cdn.plus4u.net/libs/react-dnd-touch-backend/0.7.1/Touch.browserified.js"
      },
      "react-dnd-html5-backend": {
        "globalVariable": "ReactDnDHTML5Backend",
        "uri": "https://cdn.plus4u.net/libs/react-dnd-html5-backend/7.0.2/ReactDnDHTML5Backend.min.js"
      },
      "uu_plus4u5g01": {
        "globalVariable": "Plus4U5",
        "uri": ""
      },
      "uu_plus4u5g01-app": {
        "globalVariable": "Plus4U5.App",
        "uri": ""
      },
      "uu_plus4u5g01-bricks": {
        "globalVariable": "Plus4U5.Bricks",
        "uri": ""
      },
      "uu_plus4u5g01-uubmldraw": {
        "globalVariable": "Plus4U5.UuBmlDraw",
        "uri": ""
      },
      "uu_plus4u5g01-hooks": {
        "globalVariable": "Plus4U5.Hooks",
        "uri": ""
      },
      "uu_plus4u5g01-context": {
        "globalVariable": "Plus4U5.Context",
        "uri": ""
      }
    },
    "devServerMiddlewares": [
      "uu5g04/tools/dev-server/doc-middleware.js"
    ]
  },
  "devDependencies": {
    "uu_appg01": "^5.0.0",
    "uu_appg01_devkit": "^4.0.0",
    "uu_plus4u5g02": "^0.2.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "clean": "uu_appg01_devkit clean",
    "test": "uu_appg01_devkit test",
    "build": "node tools/prebuild.js && uu_appg01_devkit build",
    "postbuild": "node tools/postbuild.js",
    "package": "uu_appg01_devkit package && node tools/postpackage.js",
    "postpackage": "node tools/postpackage.js",
    "dist": "npm run package",
    "start": "uu_appg01_devkit start",
    "prettify": "uu_appg01_devkit prettify",
    "generate": "uu_appg01_devkit generate",
    "uuAppDevKitUpgrade": "uu_appg01_devkit uuAppDevKitUpgrade",
    "docUpdateExamples": "node node_modules/uu5g04/tools/doc-cmd/update.js examples",
    "docUpdateSources": "node node_modules/uu5g04/tools/doc-cmd/update.js sources",
    "docUploadFiles": "node node_modules/uu5g04/tools/doc-cmd/update.js files"
  },
  "main": "index.js",
  "prettier": {
    "printWidth": 120
  },
  "jest": {
    "moduleNameMapper": {
      "^uu5g04-([^/?!]+)": "uu5g04/dist-node/$1/$1-build.js",
      "^uu_plus4u5g01$": "<rootDir>/src/core/core.js",
      "^uu_plus4u5g01-([^/?!]+)": "<rootDir>/src/$1/$1-build.js",
      "^(uu_appg01_oidc)$": "$1/dist/$1.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@codebase.plus4u.net:9422/uu_plus4u5g01.git"
  },
  "typings": "index.d.ts"
}